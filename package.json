    "html-webpack-plugin": "^3.2.0",
    "html-webpack-root-plugin": "^0.10.0",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "imagemin": "^7.0.1",
